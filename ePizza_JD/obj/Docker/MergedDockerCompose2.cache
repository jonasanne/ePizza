services:
  apigateway:
    build:
      context: E:\VS2019\projects\FSWD\ePizza\ePizza_JD
      dockerfile: APIGateway/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: APIGateway
    container_name: APIGateway_1
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: jonasanne/apigateway
    labels:
      com.microsoft.visual-studio.project-name: APIGateway
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/APIGateway.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - C:\Users\Rick\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\Rick\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\Rick\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
